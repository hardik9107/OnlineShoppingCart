<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app
  PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN"
  "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
<web-app>
  <display-name>ServletJDBCG4</display-name>
  <welcome-file-list>
    <welcome-file>Index.html</welcome-file>
  </welcome-file-list>
  <servlet>
	<servlet-name>FetchItems</servlet-name>
	<servlet-class>cart.FetchItemsServlet</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>AddItems</servlet-name>
	<servlet-class>cart.AddItemsServlet</servlet-class>
  </servlet>
  <servlet>
	<servlet-name>Decision</servlet-name>
	<servlet-class>cart.DecisionServlet</servlet-class>
  </servlet>
  <servlet-mapping>
	<servlet-name>FetchItems</servlet-name>
	<url-pattern>/fetchItems</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>AddItems</servlet-name>
	<url-pattern>/addItems</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
	<servlet-name>Decision</servlet-name>
	<url-pattern>/decision</url-pattern>
  </servlet-mapping>
</web-app>